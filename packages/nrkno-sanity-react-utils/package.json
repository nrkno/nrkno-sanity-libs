{
  "name": "@snorreeb/nrkno-sanity-react-utils",
  "version": "2.0.0-v3-studio.0",
  "description": "",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./build/src/index.d.ts",
      "source": "./src/index.ts",
      "import": "./build/index.esm.js",
      "require": "./build/index.js",
      "default": "./build/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "source": "./src/index.ts",
