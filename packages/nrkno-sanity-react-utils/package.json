    "access": "public"
  },
  "scripts": {
    "clean": "cross-env rimraf build *.tsbuildinfo",
    "prebuild": "npm run clean && plugin-kit verify-package --silent && pkg-utils",
    "build": "pkg-utils build",
    "test": "cross-env NODE_ENV=test jest",
    "lint": "eslint .",
    "watch": "pkg-utils watch",
    "link-watch": "plugin-kit link-watch",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nrkno/nrkno-sanity-libs.git",
    "directory": "packages/nrkno-sanity-react-utils"
  },
  "files": [
    "src",
    "build"
  ],
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.4"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^1.17.2",
    "@sanity/plugin-kit": "^2.1.7",
    "@testing-library/react": "^13.4.0",
    "@types/react-dom": "^18",
    "prettier": "^2.7.1",
    "react": "^18",
    "react-dom": "^18",
    "rimraf": "^3.0.2",
    "sanity": "3.0.0-rc.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "dev-preview || 3.0.0-rc.0"
  },
  "volta": {
    "node": "14.17.5",
    "npm": "6.14.14"
  },
  "engines": {
    "node": ">=14"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "sanityV2Json": false,
      "babelConfig": false
    },
    "linkWatch": {
      "folder": "build"
    }
  }
